<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="600.0" prefWidth="1200.0" styleClass="app" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="main.java.Controllers.ExplainListController">
    <stylesheets>
        <URL value="@/src/main/resources/css/main.css" />
    </stylesheets>
    <center>
      <TableView fx:id="explainTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="explainClm" prefWidth="1150.0" text="Explain" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" prefHeight="56.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
         <children>
            <Button onAction="#onCloseExplainTable" mnemonicParsing="false" text="Close" />
         </children>
         <padding>
            <Insets right="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
